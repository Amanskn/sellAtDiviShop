<h1>Store Page</h1>
<%=currentSeller%>

<form action="">
    <fieldset>
        <legend><h3>Store Info</h3></legend>
        <label for="address">Address :-</label>
        <input type="text" placeholder="Enter store address..." id="address" name="address">
        <br>
        <br>
        <label for="gst">GST NO. :-</label>
        <input type="text" placeholder="Enter GST number..." id="gst" name="gst">
        <br>
        <br>
        <label for="logo">Logo :-</label>
        <input type="file" placeholder="Enter Store logo..." id="logo" name="logo">
        <br>
        <br>
        <label for="from-time">From :-</label>
        <input type="time" placeholder="Enter opening time..." id="from-time" name="fromTime">
        <br>
        <br>
        <label for="to-time">To :-</label>
        <input type="time" placeholder="Enter closing time..." id="to-time" name="toTime">
    </fieldset>
</form>
